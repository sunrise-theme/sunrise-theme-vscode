      "settings": {
        "foreground": "{{ syntax.scope.entity.r-00-red-xxxxxxxxxxxxxxxx }}"
      }
    },
    {
      "name": "Text pug",
      "scope": "text.pug",
      "settings": {
        "foreground": "{{ syntax.scope.text.wb-00-white-xxxxxxxxxxxxx }}"
      }
    },
    {
      "name": "JS type class",
      "scope": "entity.name.type.class.js",
      "settings": {
        "foreground": "{{ syntax.scope.entity.r-16-bright-blue-xxxxxxxx }}"
      }
    },
    {
      "name": "JS control as",
      "scope": "keyword.control.as.js",
      "settings": {
        "foreground": "{{ syntax.scope.keyword.r-05-bright-magenta-xxxxx }}"
      }
    },
    {
      "name": "JS control from",
      "scope": "keyword.control.from.js",
      "settings": {
        "foreground": "{{ syntax.scope.keyword.r-05-bright-magenta-xxxxx }}"
      }
    },
    {
      "name": "JS control export",
